{
  "name": "@wywywywy/tesla-prometheus-exporter",
  "version": "1.0.8",
  "description": "Prometheus exporter for Tesla stats",
  "main": "dist/tesla-prometheus-exporter.js",
  "repository": "git@github.com:wywywywy/tesla-prometheus-exporter.git",
  "author": "W Leung <wywywywy@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "npm run watch",
    "build": "npm run clean && npm run build:ts",
    "test": "jest --forceExit --coverage --verbose",
    "watch:test": "npm run run test -- --watchAll",
    "build:ts": "tsc",
    "lint": "tsc --noEmit && eslint \"src/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,mocks}__}/**/*.ts\"",
    "clean": "rm -rf dist/",
    "watch": "ts-node-dev --respawn src/tesla-prometheus-exporter.ts"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "bin": {
    "tesla-prometheus-exporter": "./dist/tesla-prometheus-exporter.js",
    "tesla": "./dist/bin/tesla.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.0",
    "@types/node": "^11.15.3",
    "@types/node-fetch": "^2.5.8",
    "@types/yargs": "^15.0.12",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "node-fetch": "^2.6.7",
    "prom-client": "^13.1.0",
    "typescript": "^4.5.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.5.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.1.8"
  }
}
